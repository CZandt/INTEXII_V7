@{
    ViewData["Title"] = "Summary";
}
@model List<Burialmain>;

<h1>Summary Placeholder</h1>

<table class="table table-sm table-bordered">
    <tr>
        <th>id</th>
        <th>Text</th>
        <th>Area</th>
        <th>Depth</th>
    </tr>

    @foreach(Burialmain x in Model) {
        <tr>
            <td>@x.Id</td>
            <td>@x.Text </td>
            <td>@x.Area</td>
            <td>@x.Depth</td>
        </tr>
    }
</table>