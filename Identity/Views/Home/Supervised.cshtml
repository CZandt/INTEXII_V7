@{
    ViewData["Title"] = "Supervised";
}
@model MummyData


<div class="card">
    <form method="post" asp-action="Supervised">
        <div class="card-header">
            <h3>Predict Head Direction</h3>
        </div>
        <div class="card-body">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="length">Length</label>
                <input asp-for="length" class="form-control"/>
            </div>

            <div class="form-group">
                <label asp-for="depth">Depth</label>
                <input asp-for="depth" class="form-control" />
            </div>

            <div class="form-group">
                <label asp-for="squarenorthsouth">Square North South</label>
                <input asp-for="squarenorthsouth" class="form-control" />
            </div>

            <div class="form-group">
                <label asp-for="southtohead">South to Head</label>
                <input asp-for="southtohead" class="form-control" />
            </div>

            <div class="form-group">
                <label asp-for="squareeastwest">Square East West</label>
                <input asp-for="squareeastwest" class="form-control" />
            </div>

            <div class="form-group">
                <label asp-for="westtohead">West to Head</label>
                <input asp-for="westtohead" class="form-control" />
            </div>

            <div class="form-group">
                <label asp-for="westtofeet">West to Feet</label>
                <input asp-for="westtofeet" class="form-control" />
            </div>

            <div class="form-group">
                <label asp-for="southtofeet">South to Feet</label>
                <input asp-for="southtofeet" class="form-control" />
            </div>

            <div class="float-end">
                <input class="btn btn-primary" type="submit" value="Predict Head Direction" />
            </div>
        </div>


    </form>
   
</div>

